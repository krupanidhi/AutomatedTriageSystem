    },
    "Gemini": {
      "ApiKey": "YOUR_GEMINI_API_KEY_HERE",
      "Model": "gemini-2.5-flash",
      "DelayBetweenCallsMs": 60000
    },
    "Claude": {
      "ApiKey": "YOUR_CLAUDE_API_KEY_HERE",
